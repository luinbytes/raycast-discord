{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "discord",
  "title": "Discord",
  "description": "Fully integrate Discord into your RayCast!",
  "icon": "extension-icon.png",
  "license": "MIT",
  "commands": [
    {
      "name": "link-account",
      "title": "Link Account",
      "description": "Link discord to raycast",
      "mode": "view",
      "subtitle": ""
    },
    {
      "name": "dashboard",
      "title": "Dashboard",
      "description": "Discord Dashboard",
      "mode": "view"
    },
    {
      "name": "dms",
      "title": "Direct Messages",
      "description": "View DMs",
      "mode": "view"
    },
    {
      "name": "guilds",
      "title": "Guilds",
      "description": "View Servers",
      "mode": "view"
    },
    {
      "name": "spy-mode",
      "title": "Live Spy Mode",
      "description": "Monitor a user",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.103.0",
    "@raycast/utils": "^2.2.1",
    "discord.js-selfbot-v13": "^3.7.1",
    "ffmpeg-static": "^5.3.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "dev": "ray develop",
    "lint": "ray lint",
    "fix-lint": "ray lint --fix",
    "build": "ray build",
    "publish": "npx @raycast/api@latest publish",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1"
  },
  "author": "nazzy_wazzy_lu",
  "platforms": [
    "Windows"
  ],
  "categories": [
    "Applications"
  ]
}
